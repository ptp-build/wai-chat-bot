{
  "name": "wai-chat-bot",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev:nodemon": "nodemon",
    "start": "ts-node src/bot/server.ts",
    "start:bot": "ts-node src/bot.ts",
    "dev": "wrangler dev src/worker/index.ts --port 2235 --local --persist-to=./wrangler-local-state",
    "deploy:worker-websocket": "wrangler publish src/worker/worker_websocket.ts -c wrangler.websocket.prod.toml",
    "deploy:worker-master": "wrangler publish src/worker/worker_master.ts --name=wai-chat-bot-master -c wrangler.prod.toml",
    "deploy:worker-chatgpt": "wrangler publish src/worker/worker_chatgpt.ts --name=wai-chat-bot-chatgpt -c wrangler.prod.toml",
    "deploy:worker-storage": "wrangler publish src/worker/worker_storage.ts --name=wai-chat-bot-storage -c wrangler.prod.toml",
    "deploy:worker": "wrangler publish src/worker/index.ts --name=wai-chat-bot -c wrangler.websocket.prod.toml",
    "logs:worker": "wrangler tail --format=pretty",
    "logs:worker:websocket": "wrangler tail --format=pretty"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@cloudflare/itty-router-openapi": "^0.0.10",
    "axios": "^1.3.6",
    "bip39": "^3.0.4",
    "bn.js": "^4.12.0",
    "crypto-browserify": "^3.12.0",
    "elliptic": "^6.5.4",
    "ethereum-cryptography": "0.1.3",
    "ethereumjs-wallet": "^1.0.2",
    "eventsource-parser": "^1.0.0",
    "express": "^4.16.3",
    "node-fetch": "^3.3.1",
    "openai": "^3.2.1",
    "password-validator": "^5.3.0",
    "pettier": "^1.1.0",
    "protobufjs": "^7.2.2",
    "request": "^2.88.2",
    "worktop": "0.8.0-next.14",
    "ws": "^5.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.0.0",
    "@jest/console": "^29.2.0",
    "@types/bn.js": "^5.1.1",
    "@types/elliptic": "^6.4.14",
    "@types/jest": "^29.5.1",
    "@types/ws": "^8.5.4",
    "dotenv": "^16.0.3",
    "jest": "^29.2.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "wrangler": "^2.15.0"
  }
}
